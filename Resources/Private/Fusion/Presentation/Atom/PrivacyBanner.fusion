prototype(VIVOMEDIA.PrivacyShield:Atom.PrivacyBanner) < prototype(Neos.Fusion:Component) {

    content = 'privacy-banner'

    renderer = afx`
        <div class={props.class} style="position: absolute; bottom: 0; background-color: rgba(255, 255, 255,0.8);">
            {props.content}
        </div>
    `
}